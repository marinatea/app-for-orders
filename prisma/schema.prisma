datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int    @id @default(autoincrement())
  userId   String @unique
  userName String?
  role     String
}

model Product {
  id          String   @id @default(cuid())
  name        String
  orderLink   String
  description String?
  category    String?
  store       String?
}
